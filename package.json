{
  "name": "GitPie",
  "version": "0.1.0",
  "description": "A Multiplataform Client for Git",
  "main": "index.html",
  "window": {
    "title": "GitPie",
    "icon": "resources/images/icon.png",
    "toolbar": false,
    "frame": true,
    "width": 1000,
    "height": 600,
    "min_width": 1000,
    "min_height": 600,
    "resizable": true
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run linux64",
    "linux32": "grunt build && ./build/GitPie/linux32/GitPie",
    "linux64": "grunt build && ./build/GitPie/linux64/GitPie",
    "osx32": "grunt build && ./build/GitPie/osx32/GitPie",
    "osx64": "grunt build && ./build/GitPie/osx64/GitPie",
    "win32": "grunt build && ./build/GitPie/win32/GitPie",
    "win64": "grunt build && ./build/GitPie/win64/GitPie",
    "dev": "grunt dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mapaiva/GitPie.git"
  },
  "keywords": [
    "GitHub",
    "BitBucket",
    "Git",
    "Client",
    "Linux",
    "Windows",
    "Mac",
    "GUI"
  ],
  "author": "Matheus Paiva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapaiva/GitPie/issues"
  },
  "homepage": "https://github.com/mapaiva/GitPie",
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-nw-builder": "^2.0.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "fs-extra": "^0.24.0",
    "git-repo-name": "^0.3.1",
    "git-url-parse": "^4.0.0",
    "node-wos": "^0.1.0",
    "request": "^2.61.0",
    "tar.gz": "^1.0.0"
  }
}
