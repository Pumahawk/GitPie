{
  "name": "GitPie",
  "version": "0.1.0",
  "description": "A Multiplataform Client for Git",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "dev": "gulp dev",
    "build": "gulp build",
    "linux32": "gulp build:linux32 && ./build/linux/32bit/GitPie-linux-ia32/GitPie",
    "linux64": "gulp build:linux64 && ./build/linux/64bit/GitPie-linux-x64/GitPie",
    "osx32": "gulp build:osx32 && ./build/osx/32bit/GitPie-darwin-ia32/GitPie.app",
    "osx64": "gulp build:osx64 && ./build/osx/64bit/GitPie-darwin-x64/GitPie.app",
    "win32": "gulp build:win32 && build\\windows\\32bit\\GitPie-win32-ia32\\GitPie.exe",
    "win64": "gulp build:win64 && build\\windows\\64bit\\GitPie-win32-x64\\GitPie.exe"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mapaiva/GitPie.git"
  },
  "keywords": [
    "GitHub",
    "BitBucket",
    "Git",
    "Client",
    "Linux",
    "Windows",
    "Mac",
    "GUI"
  ],
  "author": "Matheus Paiva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapaiva/GitPie/issues"
  },
  "homepage": "https://github.com/mapaiva/GitPie",
  "devDependencies": {
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.35.1",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.1.1"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "electronify": "^1.1.0",
    "fs-extra": "^0.26.2",
    "git-url-parse": "^4.0.0",
    "node-wos": "^0.1.0",
    "request": "^2.61.0",
    "tar.gz": "^1.0.0"
  }
}
